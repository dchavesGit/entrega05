<div style="display:flex; gap: 5rem;">
  <div>

    <h1 id="main_title">Productos agregados</h1>
    <div id="container">
      {{#each products}}
        <ul id="{{this.id}}">
          <li>{{this.title}}</li>
          <li>{{this.description}}</li>
          <li>{{this.code}}</li>
          <li>{{this.price}}</li>
          <li>{{this.status}}</li>
          <li>{{this.stock}}</li>
          <li>{{this.category}}</li>
          <li>{{this.thumbnail}}</li>
          <li>{{this.id}}</li>
        </ul>
      {{/each}}
    </div>
  </div>
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/realTimeProducts.js"></script>